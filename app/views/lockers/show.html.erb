<div class="container" id="show">
  <div class="row">
    <div class="col">
      <div class="card" style="width: 100%;">
        <img src="https://source.unsplash.com/8528gpeukK8" class="card-img-top" alt="...">
        <div class="card-body">
          <h5 class="card-title">Lockers Title</h5>
          <p class="card-text">Locker Description</p>
        </div>
      </div>
    </div>

    <div class="col">
      <form>
        <div class="form-group simple">
          <%= simple_form_for :search, method: "get" do |f| %>
            <%= f.input :starts_at, as: :string, required: false, input_html: {class: "datepicker"} %>
            <%= f.input :ends_at, as: :string, required: false, input_html: {class: "datepicker"} %>
            <%= f.button :submit, "Reserve", class: "btn btn-primary" %>
          <% end %>
        </div>
      </form>
    </div>
  </div>

  <div class="row" id="show-map">
    <div class="col">
      <div class="map-responsive">
        <iframe src="https://www.google.com/maps/embed/v1/place?key=AIzaSyA0s1a7phLN0iaD6-UE7m4qP-z21pH0eSc&q=Eiffel+Tower+Paris+France" width="538" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
      </div>
    </div>
  </div>
</div>
