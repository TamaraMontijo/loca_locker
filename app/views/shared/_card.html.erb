 <div class="cards">
  <% @lockers.each do |locker| %>
    <%= link_to locker_path(locker) do %>
    <div class="card-category" id="small-locker" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(<%= locker.photo %>)">
      <%= locker.size %>
      <%= locker.price %>
      <%= locker.address %>
   </div>
    <% end %>
  <% end %>
</div>

